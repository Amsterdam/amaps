{
  "name": "amaps",
  "version": "1.0.0",
  "description": "implementation of nlmaps for Amsterdam",
  "main": "src/index.js",
  "browser": "dist/amaps.iife.js",
  "nlmaps_version": "2.3.17",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "proj4": "^2.4.4",
    "stijl": "git+https://git@github.com/Amsterdam/stijl.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "connect": "^3.6.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "http-server": "^0.11.1",
    "live-server": "^1.2.0",
    "live-server-https": "0.0.2",
    "nodemon": "^1.17.4",
    "pa11y": "^5.0.3",
    "puppeteer": "^1.3.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "serve-static": "^1.13.2",
    "shx": "^0.2.2",
    "tap": "^11.1.4"
  },
  "scripts": {
    "test": "node scripts/test",
    "watch-test": "nodemon --watch scripts --watch test -e js,html scripts/serve.js",
    "watch-local-test": "nodemon --watch scripts --watch test -e js,html scripts/test.js",
    "serve": "http-server test",
    "lint": "eslint test scripts src",
    "build-amaps": "shx mkdir -p dist && NODE_ENV=production rollup -c rollup.config.js && shx cp -r node_modules/stijl/dist/* dist/",
    "build-amaps-dev": "NODE_ENV=development rollup -c rollup.config.js && cp -r node_modules/stijl/dist test/",
    "watch-build-amaps-dev": "NODE_ENV=development rollup -w -c rollup.config.js",
    "docker-test": "node scripts/docker-test",
    "watch-docker-test": "nodemon --watch scripts --watch test -e js,html scripts/docker-test.js",
    "get-nlmaps": "shx rm -rf nlmaps && git clone --depth=1 --branch v$npm_package_nlmaps_version https://github.com/webmapper/nlmaps && shx ln -sf ../nlmaps test/nlmaps",
    "install-nlmaps": "node scripts/install-nlmaps.js",
    "build-nlmaps": "node scripts/build-nlmaps.js",
    "watch-nlmaps": "nodemon --watch config --exec npm run build-nlmaps",
    "nlmaps": "npm run get-nlmaps && npm run install-nlmaps && npm run build-nlmaps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webmapper/amaps.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webmapper/amaps/issues"
  },
  "homepage": "https://github.com/webmapper/amaps#readme"
}
